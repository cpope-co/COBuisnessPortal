{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "types": [
      "cypress",
      "node"
    ],
    "esModuleInterop": true,
    "isolatedModules": false,
    "moduleResolution": "node",
    "typeRoots": [
      "../node_modules/@types",
      "./support"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "support/index.d.ts"
  ],
  "exclude": []
}
